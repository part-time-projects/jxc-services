<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.finer.erp.jeecg.bas.mapper.BasMaterialSupplierPriceMapper">

    <select id="queryMaterialIds" resultType="java.lang.String" parameterType="java.lang.String">
        select any_value(material_id) from bas_material_supplier_price
        <where>
            <if test="supplierId != null and supplierId != ''">
                supplier_id = #{supplierId}
            </if>
        </where>
        group by supplier_id
    </select>


</mapper>